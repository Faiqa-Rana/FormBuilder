{
    "version": 2,
    "projects": [
      {
        "src": "client/",
        "output": "/"
      },
      {
        "src": "server/",
        "output": "/api"
      }
    ],
    "rewrites": [
      { "source": "/api/(.*)", "destination": "/server/$1" },
      { "source": "/(.*)", "destination": "/client/$1" }
    ]
  }
  